{
  "name": "react-browser-support-copy",
  "version": "0.0.0-development",
  "description": "A React component indicating whether or not your application supports the detected browser ( & semantic version ).",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "webpack -p --colors -w",
    "deploy": "npm unlink && npm publish",
    "build": "webpack -p",
    "prepublish": "npm run build",
    "test": "jest",
    "semantic-release": "semantic-release && npm publish",
    "travis-deploy-once": "travis-deploy-once",
    "commit": "git cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noelalfonsomiranda/react-browser-support-copy.git"
  },
  "keywords": [
    "react",
    "browser",
    "support",
    "version",
    "detect",
    "compatibility"
  ],
  "author": "Noel Alfonso Miranda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noelalfonsomiranda/react-browser-support-copy/issues"
  },
  "homepage": "https://github.com/noelalfonsomiranda/react-browser-support-copy#readme",
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "commitizen": "2.10.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "2.8.2",
    "enzyme-adapter-react-15": "^1.0.6",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^23.4.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "15.6.1",
    "sass-loader": "^6.0.6",
    "semantic-release": "^15.8.1",
    "style-loader": "^0.18.2",
    "travis-deploy-once": "^5.0.1",
    "webpack": "^3.4.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "detect-browser": "^2.0.0",
    "node-sass": "4.9.2",
    "semver-compare": "^1.0.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>",
      "\\.(css|less|scss|sass)$": "<rootDir>"
    }
  }
}
